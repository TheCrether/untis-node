{
	"name": "untis-node",
	"version": "1.0.2",
	"description": "A package for communicating with the Webuntis API",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"start": "ts-node src/index.ts",
		"start:dev": "ts-node src/test.ts",
		"dev": "nodemon",
		"build": "tsc",
		"format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
		"lint": "tslint -p tsconfig.json",
		"prepare": "npm run build && npm run docs",
		"prepublishOnly": "npm test && npm run lint",
		"preversion": "npm run lint",
		"version": "npm run format && git add -A src",
		"postversion": "git push && git push --tags",
		"docs": "typedoc --out docs --readme ./README.md src"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/TheCrether/untis-node.git"
	},
	"keywords": [
		"webuntis",
		"untis",
		"typescript"
	],
	"author": "TheCrether",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/TheCrether/untis-node/issues"
	},
	"homepage": "https://github.com/TheCrether/untis-node",
	"devDependencies": {
		"@types/axios": "^0.14.0",
		"prettier": "^1.17.1",
		"ts-node": "^8.1.0",
		"tslint": "^5.17.0",
		"tslint-config-prettier": "^1.18.0",
		"typescript": "^3.4.5"
	},
	"dependencies": {
		"@types/node": "^12.0.3",
		"axios": "^0.19.0"
	},
	"files": [
		"dist/**/*"
	]
}